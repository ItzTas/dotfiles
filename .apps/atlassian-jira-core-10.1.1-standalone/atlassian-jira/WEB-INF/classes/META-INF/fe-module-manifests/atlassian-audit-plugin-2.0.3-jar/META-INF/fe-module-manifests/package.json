    "scripts": {
        "//": "echo Remove once the PR to upgrade to webpack 5 goes through",
        "analyse": "NODE_OPTIONS=--openssl-legacy-provider webpack --config ./webpack.bundleAnalyser.config.js",
        "build": "NODE_OPTIONS=--openssl-legacy-provider webpack --mode=production --config ./webpack.production.config.js && cp -r build/classes ../target",
        "checkstyle:ci": "npm-run-all2 --parallel eslint:ci prettier:ci tslint tscheck lockfile-lint:ci",
        "checkstyle:fix": "yarn eslint:fix && yarn prettier:fix && yarn tslint:fix",
        "coverage": "nyc report --reporter=text",
        "cypress:it": "cypress run --env coverage=false",
        "cypress:it:chrome": "cypress run --browser chrome --headless",
        "cypress:it:firefox": "cypress run --browser firefox --headless",
        "cypress:it:dev": "CYPRESS_BASE_URL=http://localhost:5990/refapp/ cypress run --spec 'cypress/integration/login.spec.ts' --env DEV_LOOP=true",
        "cypress:open": "cypress open --env coverage=false",
        "cypress:dev": "start-server-and-test 'yarn start' http://localhost:3000/audit.bundle.js 'yarn cypress:open'",
        "eslint": "eslint '**/*.{js,jsx,ts,tsx}'",
        "eslint:fix": "yarn eslint --fix",
        "eslint:ci": "yarn eslint",
        "license-lint:ci": "solicitorio .",
        "lockfile-lint:ci": "lockfile-lint --path yarn.lock --allowed-hosts packages.atlassian.com --validate-https",
        "node-modules:fast-delete": "mkdir empty_dir && rsync -a --delete empty_dir/ node_modules && rm -r empty_dir",
        "prettier": "prettier '**/*.{js,jsx,ts,tsx,css,less,json}'",
        "prettier:fix": "yarn prettier --write",
        "prettier:ci": "yarn prettier --list-different",
        "///": "echo Remove once the PR to upgrade to webpack 5 goes through",
        "start": "NODE_OPTIONS=--openssl-legacy-provider BABEL_ENV=webpack-dev-server webpack-dev-server --config ./webpack.development.config.js --env watch",
        "start:test": "NODE_OPTIONS=--openssl-legacy-provider BABEL_ENV=webpack-dev-server webpack-dev-server --config ./webpack.development.test.config.js",
        "storybook": "NODE_OPTIONS=--openssl-legacy-provider NODE_PATH=src start-storybook -p 4000 -c .storybook -s ./__mocks__,./node_modules",
        "test:dev-loop": "start-server-and-test 'yes n | yarn refapp' http://localhost:5990/refapp 'yarn test:it:dev'",
        "test:it": "start-server-and-test 'yarn start' http://localhost:3000/audit.bundle.js 'yarn cypress:it'",
        "test:it:dev": "start-server-and-test 'yarn start' http://localhost:3000/audit.bundle.js 'yarn cypress:it:dev'",
        "test:it:chrome": "start-server-and-test 'yarn start:test' http://localhost:3000/audit.bundle.js 'yarn cypress:it:chrome'",
        "test:it:firefox": "start-server-and-test 'yarn start:test' http://localhost:3000/audit.bundle.js 'yarn cypress:it:firefox'",
        "test:ci:chrome": "start-server-and-test 'yarn start:test' http://localhost:3000/audit.bundle.js 'yarn cypress:it:chrome --reporter junit --reporter-options mochaFile=./test-reports/chrome-[hash].xml'",
        "test:ci:firefox": "start-server-and-test 'yarn start:test' http://localhost:3000/audit.bundle.js 'yarn cypress:it:firefox --env coverage=false --reporter junit --reporter-options mochaFile=./test-reports/firefox-[hash].xml'",
        "tscheck": "tsc --noEmit",
        "tslint": "tslint -c tslint.json -p tsconfig.json",
        "tslint:fix": "tslint --fix -c tslint.json -p tsconfig.json",
        "bitbucket": "cd .. && mvn amps:run -Dproduct=bitbucket -DskipTests",
        "confluence": "cd .. && mvn amps:run -Dproduct=confluence -DskipTests",
        "jira": "cd .. && mvn amps:run -Dproduct=jira -DskipTests",
        "refapp": "cd .. && mvn amps:run -Dproduct=refapp -DskipTests"
    },
