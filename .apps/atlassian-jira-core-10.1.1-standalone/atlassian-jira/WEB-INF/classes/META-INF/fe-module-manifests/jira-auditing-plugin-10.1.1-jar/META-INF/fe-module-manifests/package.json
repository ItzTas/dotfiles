{
    "name": "@atlassian/dc.jira.jira-auditing-plugin",
    "author": "Atlassian",
    "private": true,
    "version": "0.0.1",
    "repository": {
        "type": "git",
        "url": "ssh://git@stash.atlassian.com:7997/jiraserver/jira.git",
        "directory": "jira-components/jira-plugins/jira-auditing-plugin"
    },
    "devDependencies": {
        "copy-files-from-to": "3.9.0",
        "patch-package": "^8.0.0"
    },
    "dependencies": {
        "backbone.paginator": "1.0.0"
    },
    "scripts": {
        "postinstall": "patch-package --error-on-fail --error-on-warn && copy-files-from-to"
    },
    "copyFiles": [
        {
            "from": "node_modules/backbone.paginator/lib/backbone.paginator.js",
            "to": "target/classes/js/libs/backbone.paginator.js"
        },
        {
            "from": "node_modules/backbone.paginator/lib/backbone.paginator.js",
            "to": {
                "default": {
                    "dest": "target/classes/js/libs/backbone.paginator-min.js",
                    "minifyJs": true
                }
            }
        }
    ]
}
