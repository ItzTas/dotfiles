{
  "name": "@atlassian/jira-issue-search-plugin.issue-nav-plugin",
  "author": "Atlassian",
  "private": true,
  "version": "20.0.11",
  "repository": {
    "type": "git",
    "url": "https://stash.atlassian.com/scm/jiraserver/jira-issue-search-plugin.git",
    "directory": "jira-global-issue-navigator-plugin"
  },
  "dependencies": {
    "jquery-simulate": "file:src/main/frontend/jquery-simulate/0.0.1"
  },
  "devDependencies": {
    "copy-files-from-to": "3.11.0"
  },
  "copyFiles": [
    {
      "from": "node_modules/jquery-simulate/src/jquery.simulate.js",
      "to": "target/classes/libs/jquery.simulate.js"
    },
    {
      "from": "node_modules/jquery-simulate/src/jquery.simulate.js",
      "to": {
        "default": {
          "dest": "target/classes/libs/jquery.simulate-min.js",
          "minifyJs": true
        }
      }
    },
    {
      "from": "node_modules/jquery-simulate/src/jquery.simulate.delayedDrag.js",
      "to": "target/classes/libs/jquery.simulate.delayedDrag.js"
    },
    {
      "from": "node_modules/jquery-simulate/src/jquery.simulate.delayedDrag.js",
      "to": {
        "default": {
          "dest": "target/classes/libs/jquery.simulate.delayedDrag-min.js",
          "minifyJs": true
        }
      }
    }
  ],
  "scripts": {
    "postinstall": "copy-files-from-to",
    "lintCI": "echo 'TBD'"
  }
}
