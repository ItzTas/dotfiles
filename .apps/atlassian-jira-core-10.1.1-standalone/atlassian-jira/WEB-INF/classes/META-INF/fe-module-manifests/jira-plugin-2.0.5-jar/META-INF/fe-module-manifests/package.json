{
  "name": "@atlassian/dc.shared.app-usage.frontend",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://stash.atlassian.com/scm/dcq/app-usage-insights.git",
    "directory": "frontend"
  },
  "license": "SEE LICENSE.TXT",
  "scripts": {
    "build": "cross-env NODE_OPTIONS='--openssl-legacy-provider' atlas-fe build",
    "cilint": "npm-run-all2 --parallel cilint:*",
    "cilint:eslint": "eslint --max-warnings=0 --ext ts --ext tsx --ext js .",
    "cilint:licenses": "solicitorio .",
    "cilint:lockfile": "yarn lockfile-lint",
    "cilint:package-json": "yarn sort-package-json --check",
    "cilint:prettier": "yarn prettier --check .",
    "format": "prettier . --write && sort-package-json",
    "install-hook": "cd .. && husky install",
    "lint": "npm-run-all2 --parallel lint:*",
    "lint:es": "npm-run-all2 --sequential lint:es:*",
    "lint:es:eslint": "eslint . --fix",
    "lint:es:prettier": "yarn prettier --write .",
    "lint:lockfile": "npm-run-all2 --parallel lint:lockfile:*",
    "lint:lockfile:hosts": "yarn lockfile-lint",
    "lint:lockfile:licenses": "solicitorio . && :",
    "lint:package-json": "yarn sort-package-json",
    "lockfile-lint": "lockfile-lint --path yarn.lock --allowed-hosts packages.atlassian.com --validate-https",
    "prepare-confluence": "cd ../confluence-plugin && yarn prepare-dev && mvn clean install -Dmaven.test.skip=true",
    "start-confluence": "yarn prepare-confluence && NODE_OPTIONS='--openssl-legacy-provider' PLUGIN_KEY='com.atlassian.app.usage.confluence-plugin' atlas-fe start -Pconfluence",
    "start:prepare": "atlas-debug -U -DwatchMode -Dmaven.test.skip=true",
    "start:watch": "atlas-fe watch",
    "start:watch-prepare": "NODE_OPTIONS='--openssl-legacy-provider' atlas-fe watch-prepare"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --max-warnings=0 --fix"
    ],
    "*.{ts,tsx,js,json}": [
      "prettier --write"
    ],
    "yarn.lock": "yarn lockfile-lint",
    "package.json": "sort-package-json",
    "(yarn.lock|package.json)": "yarn run lint:lockfile:licenses"
  },
  "babel": {
    "plugins": [
      [
        "@atlaskit/tokens/babel-plugin",
        {
          "shouldUseAutoFallback": true
        }
      ]
    ],
    "presets": [
      "module:@atlassian/atlas-fe/presets/babel",
      "module:@atlassian/atlas-fe-typescript/presets/babel",
      "module:@atlassian/atlas-fe-react/presets/babel"
    ]
  },
  "browserslist": [
    "extends @atlassian/browserslist-config-server"
  ],
  "prettier": "@atlassian/atlas-fe/presets/prettier",
  "resolutions": {
    "atlassian-webresource-webpack-plugin": "^4.10.0",
    "chokidar": "^3.6.0"
  },
  "dependencies": {
    "@atlaskit/button": "^16.18.1",
    "@atlaskit/dynamic-table": "^14.11.11",
    "@atlaskit/empty-state": "^7.10.1",
    "@atlaskit/flag": "^15.7.4",
    "@atlaskit/icon": "^21.12.8",
    "@atlaskit/inline-message": "^11.5.8",
    "@atlaskit/locale": "^2.7.0",
    "@atlaskit/lozenge": "^11.9.1",
    "@atlaskit/modal-dialog": "^12.14.9",
    "@atlaskit/page-header": "^10.8.0",
    "@atlaskit/page-layout": "^1.8.4",
    "@atlaskit/pagination": "^14.7.10",
    "@atlaskit/popup": "^1.21.0",
    "@atlaskit/primitives": "^1.20.0",
    "@atlaskit/section-message": "^6.5.10",
    "@atlaskit/spinner": "^15.6.1",
    "@atlaskit/table": "^0.6.9",
    "@atlaskit/tabs": "^13.4.9",
    "@atlaskit/theme": "^12.11.2",
    "@atlaskit/tokens": "^0.13.5",
    "@atlaskit/tooltip": "^17.8.10",
    "@atlaskit/visually-hidden": "^1.4.1",
    "@atlassian/wrm-react-i18n": "^2.0.1",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "dompurify": "^3.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "@atlassian/atlas-fe": "^0.3.3",
    "@atlassian/atlas-fe-clientside-extensions": "^0.3.3",
    "@atlassian/atlas-fe-prettier": "^0.1.1",
    "@atlassian/atlas-fe-react": "^0.3.3",
    "@atlassian/atlas-fe-typescript": "^0.3.3",
    "@atlassian/eslint-config-atlas-fe": "^0.3.3",
    "@atlassian/solicitorio": "^3.2.0",
    "@types/dompurify": "^3.0.5",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/styled-components": "^5.1.34",
    "@types/when-dom-ready": "^1.2.2",
    "atlassian-webresource-webpack-plugin": "^4.10.0",
    "cross-env": "^7.0.3",
    "css-loader": "3.6.0",
    "eslint": "^7.32.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "file-loader": "^6.2.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "lockfile-lint": "^4.14.0",
    "npm-run-all2": "^6.1.2",
    "sort-package-json": "^2.6.0",
    "style-loader": "1.3.0",
    "typescript": "^4.9.5"
  }
}
