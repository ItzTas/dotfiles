  "main": "no-entry.js",
  "scripts": {
    "ci": "rm -rf node_modules && yarn install --frozen-lockfile --network-timeout 600000",
    "lint": "eslint .",
    "format": "prettier . --write && eslint . --fix",
    "build": "webpack --mode=production && yarn build:static",
    "build:dev": "webpack --mode=development && yarn build:static",
    "build:watch-mode": "webpack --mode=development --env=watch-mode && yarn build:static",
    "build:watch": "yarn run build:dev -- --watch",
    "build:static": "copy-files-from-to",
    "prepare": "cd .. && husky install",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "test:bamboo": "yarn lint && yarn type-check && JEST_JUNIT_OUTPUT_DIR=target/jest JEST_JUNIT_OUTPUT_NAME=test-results.xml jest --ci --reporters=default --reporters=jest-junit --debug --runInBand",
    "type-check": "tsc --noEmit",
    "checks": "yarn lint && yarn type-check && yarn test && yarn lint:unused-exports",
    "dev-server": "webpack-dev-server --mode=development --env=dev-server",
    "start": "mvn package -DskipTests -Pwatch-mode && yarn run dev-server",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint:test-exists": "node ./scripts/lint-test-exists/lint-test-exists.js",
    "lint:unused-exports": "ts-prune frontend"
  },
  "copyFiles": [
    {
      "from": "node_modules/js-cookie/src/js.cookie.js",
      "to": "target/classes/js/lib/js.cookie.js"
    }
  ],
