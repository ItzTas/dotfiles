{
    "allowDuplicates": false,
    "historyFile": "clipboard_history.json",
    "maxHistory": 1000,
    "logFile": "clipse.log",
    "themeFile": "themes/lavender.json",
    "tempDir": "tmp_files",
    "keyBindings": {
        "choose": "ctrl+y",
        "clearSelected": "C",
        "up": "up",
        "down": "down",
        "end": "end",
        "filter": "/",
        "home": "home",
        "more": "?",
        "nextPage": "right",
        "prevPage": "left",
        "preview": " ",
        "quit": "q",
        "remove": "d",
        "selectUp": "ctrl+k",
        "selectDown": "ctrl+j",
        "selectSingle": "v",
        "togglePin": "p",
        "togglePinned": "tab",
        "yankFilter": "ctrl+s"
    },
    "imageDisplay": {
        "type": "kitty",
        "scaleX": 9,
        "scaleY": 9,
        "heightCut": 2
    }
}
